{
  "runs": [
    {
      "workflow": ".github/workflows/release.yml",
      "checks": [
        {
          "name": "pinned-checkout",
          "passed": false,
          "reason": "checkout action not pinned to commit hash"
        },
        {
          "name": "pinned-dependencies",
          "passed": false,
          "reason": "npm install allows unpinned dependencies"
        },
        {
          "name": "token-permissions",
          "passed": true,
          "reason": "tokens scoped correctly"
        }
      ]
    }
  ]
}
